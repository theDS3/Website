{
  "name": "site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "purge": "rimraf node_modules dist",
    "lint": "rimraf dist && npm-run-all --parallel lint:astro lint:ts",
    "lint:ts": "eslint \"src/**/*.{ts,tsx,astro}\" --ignore-path .eslintignore --cache",
    "lint:astro": "astro check",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore --plugin=prettier-plugin-astro --cache --ignore-unknown --write ."
  },
  "dependencies": {
    "@astrojs/check": "^0.2.0",
    "@fontsource-variable/josefin-sans": "^5.0.13",
    "astro": "^3.1.1",
    "astro-particles": "^2.10.0",
    "sass": "^1.67.0",
    "sharp": "^0.32.6",
    "tsparticles": "^2.12.0",
    "tsparticles-engine": "^2.12.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "rimraf": "^5.0.1"
  }
